{
  "name": "tree-sitter-adventlang",
  "version": "0.1.0",
  "description": "Adventlang grammar for tree-sitter",
  "repository": "https://github.com/kelleyvanevert/adventlang",
  "license": "MIT",
  "author": {
    "name": "Kelley van Evert",
    "email": "hello@klve.nl",
    "url": "https://klve.nl/"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "adventlang"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "generate": "tree-sitter generate",
    "preplay": "tree-sitter build --wasm",
    "play": "tree-sitter playground",
    "play:watch": "ls grammar.js src/scanner.c | entr -r sh -c 'tree-sitter generate && tree-sitter build --wasm && tree-sitter playground --quiet'",
    "test": "node --test bindings/node/*_test.js"
  }
}
